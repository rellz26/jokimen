local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Header = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")
local UIStroke = Instance.new("UIStroke")
local UICorner = Instance.new("UICorner")
local FeatureList = Instance.new("Frame")
local AutoFarmToggle = Instance.new("TextButton")
local AutoFarmLabel = Instance.new("TextLabel")
local Checkbox = Instance.new("Frame")

-- Parent setup
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
MainFrame.Parent = ScreenGui
Header.Parent = MainFrame
Title.Parent = Header
CloseButton.Parent = Header
UIStroke.Parent = MainFrame
UICorner.Parent = MainFrame
FeatureList.Parent = MainFrame
AutoFarmToggle.Parent = FeatureList
AutoFarmLabel.Parent = AutoFarmToggle
Checkbox.Parent = AutoFarmToggle

-- MainFrame properties
MainFrame.Size = UDim2.new(0, 300, 0, 250)
MainFrame.Position = UDim2.new(0.5, -150, 0.5, -125)
MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
MainFrame.BorderSizePixel = 0
UIStroke.Color = Color3.fromRGB(255, 255, 255)
UIStroke.Thickness = 2
UICorner.CornerRadius = UDim.new(0, 10)

-- Header properties
Header.Size = UDim2.new(1, 0, 0, 30)
Header.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Header.BorderSizePixel = 0

-- Title properties
Title.Size = UDim2.new(1, -30, 1, 0)
Title.Position = UDim2.new(0, 10, 0, 0)
Title.Text = "JUFLING PROJECT"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 14
Title.TextXAlignment = Enum.TextXAlignment.Left
Title.BackgroundTransparency = 1

-- CloseButton properties
CloseButton.Size = UDim2.new(0, 30, 1, 0)
CloseButton.Position = UDim2.new(1, -30, 0, 0)
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255, 50, 50)
CloseButton.Font = Enum.Font.GothamBold
CloseButton.TextSize = 14
CloseButton.BackgroundTransparency = 1

-- FeatureList properties
FeatureList.Size = UDim2.new(1, 0, 1, -30)
FeatureList.Position = UDim2.new(0, 0, 0, 30)
FeatureList.BackgroundTransparency = 1

-- AutoFarmToggle properties
AutoFarmToggle.Size = UDim2.new(1, -20, 0, 30)
AutoFarmToggle.Position = UDim2.new(0, 10, 0, 10)
AutoFarmToggle.BackgroundTransparency = 1
AutoFarmToggle.AutoButtonColor = false

-- AutoFarmLabel properties
AutoFarmLabel.Size = UDim2.new(1, -40, 1, 0)
AutoFarmLabel.Text = "AutoFarm"
AutoFarmLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoFarmLabel.Font = Enum.Font.Gotham
AutoFarmLabel.TextSize = 14
AutoFarmLabel.TextXAlignment = Enum.TextXAlignment.Left
AutoFarmLabel.BackgroundTransparency = 1

-- Checkbox properties
Checkbox.Size = UDim2.new(0, 20, 0, 20)
Checkbox.Position = UDim2.new(1, -30, 0.5, -10)
Checkbox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Checkbox.BorderSizePixel = 0
local isChecked = false

-- Toggle Functionality
AutoFarmToggle.MouseButton1Click:Connect(function()
    isChecked = not isChecked
    if isChecked then
        Checkbox.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        print("AutoFarm Enabled")
        -- Tambahkan fungsi AutoFarm di sini
    else
        Checkbox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        print("AutoFarm Disabled")
    end
end)

-- Close Button Functionality
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
